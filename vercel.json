{
  "functions": {
    "app/api/**/*.go": {
      "runtime": "@vercel/go"
    }
  },
  "build": {
    "env": {
      "HUGO_VERSION": "0.121.1"
    }
  },
  "installCommand": "curl -L https://github.com/gohugoio/hugo/releases/download/v$HUGO_VERSION/hugo_extended_${HUGO_VERSION}_linux-amd64.tar.gz | tar -xz && mv hugo /vercel/path0/hugo",
  "buildCommand": "/vercel/path0/hugo --minify"
}